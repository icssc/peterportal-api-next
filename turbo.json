{
  "pipeline": {
    "db:pull": {
      "cache": false
    },
    "db:generate": {
      "dependsOn": ["db:pull"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^db:generate"],
      "outputs": ["dist/**"]
    },
    "deploy": {
      "cache": false,
      "dependsOn": ["^build"],
      "env": ["AWS_REGION"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^db:pull"],
      "env": ["PORT"]
    },
    "test": {}
  }
}
