generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "linux-arm64-openssl-1.0.x"]
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

// Do not modify anything above this line unless you know what you are doing.

model GradesInstructor {
  name     String          @id
  sections GradesSection[]
}

model GradesSection {
  id           Int                @id @default(autoincrement())
  year         Int
  quarter      String
  instructors  GradesInstructor[]
  department   String
  courseNumber String
  sectionCode  Int
  gradeACount  Int
  gradeBCount  Int
  gradeCCount  Int
  gradeDCount  Int
  gradeFCount  Int
  gradePCount  Int
  gradeNPCount Int
  gradeWCount  Int
  averageGPA   Float
}

enum Quarter {
  Fall
  Winter
  Spring
  Summer1
  Summer10wk
  Summer2
}

enum WebsocSectionType {
  Act
  Col
  Dis
  Fld
  Lab
  Lec
  Qiz
  Res
  Sem
  Stu
  Tap
  Tut
}

model WebsocSectionInstructor {
  id          Int           @id @default(autoincrement())
  year        String
  quarter     Quarter
  sectionCode Int
  timestamp   DateTime
  section     WebsocSection @relation(fields: [year, quarter, sectionCode, timestamp], references: [year, quarter, sectionCode, timestamp])
  name        String
}

model WebsocSectionMeeting {
  id          Int           @id @default(autoincrement())
  year        String
  quarter     Quarter
  sectionCode Int
  timestamp   DateTime
  section     WebsocSection @relation(fields: [year, quarter, sectionCode, timestamp], references: [year, quarter, sectionCode, timestamp])
  days        Json
  startTime   Int
  endTime     Int
  buildings   Json
}

model WebsocTerm {
  year      String
  quarter   Quarter
  timestamp DateTime

  @@id([year, quarter])
  @@unique([year, quarter], name: "idx")
}

model WebsocSection {
  year          String
  quarter       Quarter
  sectionCode   Int
  timestamp     DateTime
  geCategories  Json
  department    String
  courseNumber  String
  courseNumeric Int
  instructors   WebsocSectionInstructor[]
  courseTitle   String
  sectionType   WebsocSectionType
  units         String
  meetings      WebsocSectionMeeting[]
  maxCapacity   Int
  sectionFull   Boolean
  waitlistFull  Boolean
  overEnrolled  Boolean
  cancelled     Boolean
  data          Json

  @@id([year, quarter, sectionCode, timestamp])
  @@unique([year, quarter, sectionCode, timestamp], name: "idx")
}
