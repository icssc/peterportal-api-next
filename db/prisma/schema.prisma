generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

// Do not modify anything above this line unless you know what you are doing.

model GradesInstructor {
  name     String          @id
  sections GradesSection[]
}

model GradesSection {
  id           Int                @id @default(autoincrement())
  year         Int
  quarter      String
  instructors  GradesInstructor[]
  department   String
  courseNumber String
  sectionCode  Int
  gradeACount  Int
  gradeBCount  Int
  gradeCCount  Int
  gradeDCount  Int
  gradeFCount  Int
  gradePCount  Int
  gradeNPCount Int
  gradeWCount  Int
  averageGPA   Float
}
