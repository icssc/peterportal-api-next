{
  "name": "@uc-irvine/api",
  "version": "0.10.8",
  "private": true,
  "description": "SDK for various UC Irvine services",
  "keywords": [],
  "homepage": "https://docs.api-next.peterportal.org",
  "bugs": {
    "url": "https://github.com/icssc/peterportal-api-next/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icssc/peterportal-api-next",
    "directory": "packages/uci-api"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./registrar": {
      "types": "./dist/registrar.d.ts",
      "import": "./dist/registrar.js",
      "require": "./dist/registrar.cjs",
      "default": "./dist/registrar.js"
    },
    "./websoc": {
      "types": "./dist/websoc.d.ts",
      "import": "./dist/websoc.js",
      "require": "./dist/websoc.cjs",
      "default": "./dist/websoc.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "registrar": [
        "./dist/registrar.d.ts"
      ],
      "websoc": [
        "./dist/websoc.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "prepublish": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "@ap0nia/camaro": "6.2.5",
    "@peterportal-api/types": "workspace:^",
    "cheerio": "1.0.0-rc.12",
    "cross-fetch": "4.0.0"
  },
  "devDependencies": {
    "tsup": "7.2.0"
  },
  "packageManager": "pnpm@8.7.4",
  "engines": {
    "node": "18",
    "pnpm": "8"
  },
  "publishConfig": {
    "access": "public"
  }
}
