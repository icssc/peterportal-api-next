{
  "name": "ant-stack",
  "version": "0.1.0",
  "description": "A serverless stack for ants",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "deploy": "cdk deploy --all --require-approval never",
    "start:cli": "node dist/cli"
  },
  "main": "dist/lambda-core.js",
  "types": "dist/lambda-core.d.ts",
  "module": "dist/lambda-core.js",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/lambda-core.d.ts",
      "import": "./dist/lambda-core.js",
      "default": "./dist/lambda-core.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "default": "./dist/cli.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js",
      "default": "./dist/config.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    }
  },
  "typesVersions": {
    "*": {
      "cli": [
        "./dist/cli.d.ts"
      ],
      "config": [
        "./dist/config.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "bin": {
    "ant-stack": "./dist/cli.js"
  },
  "dependencies": {
    "arktype": "^1.0.14-alpha",
    "aws-cdk": "^2.81.0",
    "aws-cdk-lib": "^2.53.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cleye": "^1.3.2",
    "consola": "^3.1.0",
    "constructs": "^10.1.176",
    "defu": "^6.1.2",
    "esbuild": "^0.17.19",
    "express": "^4.18.2",
    "unconfig": "^0.3.9",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.115",
    "@types/express": "^4.17.17",
    "peterportal-api-next-types": "*",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  }
}
