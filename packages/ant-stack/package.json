{
  "name": "ant-stack",
  "version": "0.1.0",
  "description": "A serverless stack for ants",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lambda-core.d.ts",
      "import": "./dist/lambda-core.js",
      "default": "./dist/lambda-core.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "default": "./dist/cli.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js",
      "default": "./dist/config.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    },
    "./constructs/*": {
      "types": "./dist/constructs/*.d.ts",
      "require": "./dist/constructs/*.cjs",
      "default": "./dist/constructs/*.js"
    }
  },
  "main": "dist/lambda-core.js",
  "module": "dist/lambda-core.js",
  "types": "dist/lambda-core.d.ts",
  "typesVersions": {
    "*": {
      "cli": [
        "./dist/cli.d.ts"
      ],
      "config": [
        "./dist/config.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ],
      "constructs/*": [
        "./dist/constructs/*.d.ts"
      ]
    }
  },
  "bin": {
    "ant-stack": "./dist/cli.js"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "deploy": "cdk deploy --all --require-approval never",
    "start:cli": "node dist/cli"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@aws-sdk/client-cloudformation": "^3.363.0",
    "@aws-sdk/util-waiter": "^3.357.0",
    "acorn": "^8.10.0",
    "aws-cdk": "^2.87.0",
    "aws-cdk-lib": "^2.87.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "ci-info": "^3.8.0",
    "cleye": "^1.3.2",
    "consola": "^3.1.0",
    "constructs": "^10.2.69",
    "cors": "^2.8.5",
    "defu": "^6.1.2",
    "esbuild": "^0.17.19",
    "express": "^4.18.2",
    "jiti": "^1.19.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.115",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "peterportal-api-next-types": "*",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  }
}
